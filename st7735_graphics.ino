Camera mainCamera = Camera();
Light mainLight = Light(Vector3(-10, 10, -5));
Device *device = 0;

const int buttonPin = 2;
long long lastTimestamp = 0;

void setup()
{
  Serial.begin(9600);
  device = new Device(80, 160, &lcd);
  const uint16_t color = 0b1111100000000000;
  
  mainCamera.position = Vector3(0, 0, 25);

  initMesh();

  pinMode(buttonPin, INPUT);

  lastTimestamp = millis();
}

int buttonState = 0;
int prevButtonState = 0;

int loopCounter = 0;

//#define USE_BUTTON
void loop()
{
  //return;
    if (loopCounter == 10)
    {
      Serial.println(millis() - lastTimestamp);
      lastTimestamp = millis();
      loopCounter = 0;
    }
    loopCounter++;
//mesh.rotation.x = 0.5;
  
  //mesh.rotation.z = 0.5;
#ifdef USE_BUTTON
  buttonState = digitalRead(buttonPin);
  //  mesh->rotation.x += 0.125;
  //  mesh->rotation.y += 0.125;
  if (buttonState == HIGH && prevButtonState == LOW)
  {
    //mesh.position.z += 1;
        mesh->rotation.y += 0.125;
    //    Serial.print(mesh->rotation.x);
    //    Serial.print("-");
    //    Serial.println(mesh->rotation.y);
    device->clearBuffer();
    device->render(mainCamera, mesh, 1, &mainLight, 1);
    device->display();
  }
  prevButtonState = buttonState;
#else
  mesh->rotation.y += 0.125;
  device->clearBuffer();
  device->render(mainCamera, mesh, 1, &mainLight, 1);
  // delay(200);
  device->display();
#endif
}
